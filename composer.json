{
  "name": "contributte/doctrine-skeleton",
  "description": "Example of first class integration of Doctrine to Nette (@nette) by @f3l1x",
  "keywords": [
    "nette",
    "skeleton",
    "project",
    "doctrine",
    "database",
    "dbal",
    "orm"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=8.1",
    "contributte/nella": "dev-master",

    "contributte/console": "^0.9.2",

    "nettrine/dbal": "dev-next as 0.9.0",
    "nettrine/orm": "dev-next as 0.9.0",
    "nettrine/migrations": "^0.8.0",
    "nettrine/fixtures": "^0.6.0",
    "nettrine/extensions-atlantic18": "^0.6.0"
  },
  "require-dev": {
    "contributte/qa": "^0.3",
    "contributte/dev": "^0.3",
    "dg/bypass-finals": "^1.4.1",
    "phpunit/phpunit": "^9.5.27",
    "mockery/mockery": "^1.5.1",
    "phpstan/phpstan": "^1.4.3",
    "phpstan/phpstan-deprecation-rules": "^1.0.0",
    "phpstan/phpstan-nette": "^1.0.0",
    "phpstan/phpstan-strict-rules": "^1.1.0",
    "phpstan/phpstan-doctrine": "^1.3.32"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
