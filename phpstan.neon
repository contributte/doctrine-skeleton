includes:
	- vendor/contributte/phpstan/phpstan.neon

parameters:
	level: 9
	phpVersion: 80100

	tmpDir: %currentWorkingDirectory%/var/tmp/phpstan

	fileExtensions:
		- php
		- phpt

	paths:
		- app
		- bin

	doctrine:
		ormRepositoryClass: App\Model\Database\Repository\AbstractRepository
		objectManagerLoader: .build/phpstan-doctrine.php
